{
  "expo": {
    "name": "Ìtáláwá",
    "slug": "Outside",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/Ìtáláwá.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash_screen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/Ìtáláwá.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.opesco.Outside"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "21cad83d-384c-4d4a-aeb4-680a632680c7"
      }
    }
  }
}
